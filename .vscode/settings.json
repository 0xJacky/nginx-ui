{
  "i18n-gettext.localesConfig": {
        "root": "app",
        "type": "nested",
        "basePath": "src/language",
        "pattern": "${locale}/${domain}.po",
        "defaultDomain": "app",
        "sourceLanguage": "en"
    },
    "i18n-gettext.translatorConfig": {
    "onlyTranslateUntranslatedAndFuzzy": true,
    "batch": {
      "pageSize": 100
    }
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/vendor": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/tmp": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/database.db": true,
    "**/.pnpm-store": true,
    "**/nginx-ui": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/tmp/**": true,
    "**/.pnpm-store/**": true
  }
}